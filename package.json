{
  "name": "personal-site",
  "version": "1.20.4",
  "description": "This is a personal branding site for Craig Sullivan",
  "author": "Craig Sullivan",
  "private": true,
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "main": "index.js",
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --config .eslintrc.json",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --config .eslintrc.json",
    "release": "HUSKY_SKIP_HOOKS=1 standard-version",
    "test": "jest --colors",
    "test:watch": "jest --colors --watch"
  },
  "keywords": [],
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "classnames": "^2.3.1",
    "clsx": "^1.1.1",
    "graphcms-image": "=1.1.0-beta4",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "isomorphic-fetch": "^3.0.0",
    "next": "^10.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-markdown": "^5.0.3",
    "react-swipeable": "^6.1.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/classnames": "^2.2.11",
    "@types/express": "^4.17.11",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-gtm-module": "^2.0.0",
    "@types/react-test-renderer": "^17.0.1",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "autoprefixer": "^10.2.5",
    "caniuse-lite": "^1.0.30001204",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "lint-staged": "^10.5.4",
    "minimist": "^1.2.5",
    "nock": "^13.0.11",
    "nodemon": "^2.0.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.32.8",
    "standard-version": "^9.1.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
