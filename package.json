{
  "name": "personal-site",
  "version": "1.14.0",
  "description": "This is a personal branding site for Craig Sullivan",
  "author": "Craig Sullivan",
  "private": true,
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "main": "index.js",
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --config .eslintrc.json",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --config .eslintrc.json",
    "release": "HUSKY_SKIP_HOOKS=1 standard-version",
    "test": "jest --colors",
    "test:watch": "jest --colors --watch"
  },
  "keywords": [],
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "classnames": "^2.2.6",
    "graphcms-image": "=1.1.0-beta4",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "isomorphic-fetch": "^3.0.0",
    "next": "^9.5.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-gtm-module": "^2.0.11",
    "react-markdown": "^4.3.1",
    "react-swipeable": "^5.5.1"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/classnames": "^2.2.10",
    "@types/express": "^4.17.8",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.5",
    "@types/react": "^16.9.51",
    "@types/react-gtm-module": "^2.0.0",
    "@types/react-test-renderer": "^16.9.3",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "autoprefixer": "^10.0.1",
    "caniuse-lite": "^1.0.30001144",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^3.0.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.2",
    "jest-canvas-mock": "^2.3.0",
    "lint-staged": "^10.4.0",
    "minimist": "^1.2.5",
    "nock": "^13.0.4",
    "nodemon": "^2.0.4",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.26.12",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
